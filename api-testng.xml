<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="HelloFreshChallenge" verbose="4" allow-return-values="true">
  <parameter name="environment" value="prod"/>
  <parameter name="platform" value="mac"/>
  <parameter name="browser" value="NA"/>
  <parameter name="is_parallel" value="false"/>
  <listeners>
    <listener class-name="com.hellofresh.challenge.commons.TestListener"/>
  </listeners>
  <test name="Api">
    <classes>
      <class name="com.hellofresh.challenge.testscripts.api.APITest"/>
    </classes>
  </test>
</suite>
